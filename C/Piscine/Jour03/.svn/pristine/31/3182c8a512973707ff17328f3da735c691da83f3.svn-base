void    my_putchar(char c);

char    *my_strchr(char *str, char c)
{
    int i;

    i = 0;
    while (*(str + i) != '\0')
    {
        if (*(str + i) == c)
        {
            return (str + i);
        }
    i++;
    }
    return (str + i);
}
